{
  "compilerOptions": {
    "incremental": true,
    "target": "esnext",
    "lib": ["es2016", "esnext"],
    "jsx": "preserve",
    "module": "commonjs",
    "allowJs": true,
    "declaration": true,
    "declarationMap": false,
    "sourceMap": false,
    "noEmit": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["index.ts"],
}
